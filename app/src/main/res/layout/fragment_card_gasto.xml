<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
                xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                tools:context=".Card_gasto">
    <android.support.v7.widget.CardView
            android:layout_width="match_parent"
            android:layout_height="213dp" android:layout_alignParentStart="true"
            android:layout_marginStart="0dp" android:layout_alignParentTop="true" android:layout_marginTop="31dp">
        <RelativeLayout
                android:layout_width="match_parent"
                android:layout_height="match_parent">

            <ImageView
                    android:src="@drawable/common_google_signin_btn_icon_dark"
                    android:layout_width="75dp"
                    android:layout_height="75dp" android:layout_alignParentStart="true"
                    android:layout_marginStart="36dp" android:layout_alignParentTop="true" android:id="@+id/imageView2"
                    android:layout_marginTop="62dp" android:layout_alignParentBottom="true"
                    android:layout_marginBottom="63dp"/>
            <ImageView
                    android:src="@drawable/common_google_signin_btn_icon_dark_focused"
                    android:layout_width="75dp"
                    android:layout_height="75dp"
                    android:layout_alignParentBottom="true"
                    android:id="@+id/imageView3" android:layout_marginBottom="60dp"
                    android:layout_toEndOf="@+id/imageView2" android:layout_marginStart="39dp"
                    android:layout_marginTop="61dp" android:layout_alignParentTop="true"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:layout_alignParentEnd="true"
                    android:layout_alignParentTop="true" android:layout_marginEnd="82dp" android:id="@+id/textView"
                    android:layout_marginTop="30dp" android:layout_toEndOf="@+id/imageView3"
                    android:layout_marginStart="44dp" android:text="Total"/>
            <TextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" android:layout_alignParentEnd="true"
                    android:layout_marginEnd="85dp" android:id="@+id/textView3"
                    android:layout_marginTop="22dp"
                    android:layout_below="@+id/textView5" android:layout_toEndOf="@+id/imageView3"
                    android:layout_marginStart="43dp" android:text="Member"/>
            <TextView
                    android:layout_width="59dp"
                    android:layout_height="24dp" android:layout_alignParentEnd="true"
                    android:layout_marginEnd="80dp" android:layout_alignParentBottom="true" android:id="@+id/textView4"
                    android:layout_marginBottom="38dp" android:layout_marginTop="22dp"
                    android:layout_below="@+id/textView3" android:layout_toEndOf="@+id/imageView3"
                    android:layout_marginStart="45dp" android:text="Date"/>
            <TextView
                    android:text="Category"
                    android:layout_width="83dp"
                    android:layout_height="wrap_content" android:layout_alignParentEnd="true"
                    android:layout_marginEnd="61dp" android:id="@+id/textView5"
                    android:layout_marginTop="22dp" android:layout_below="@+id/textView"
                    android:layout_toEndOf="@+id/imageView3" android:layout_marginStart="42dp"/>
        </RelativeLayout>
    </android.support.v7.widget.CardView>
</RelativeLayout>